#include <iostream> 
using namespace std;

int a[100];
int b[100];
int ax , ay , by;

int** matriza(int ** a, int ** b, int ax, int ay, int by)
{
	int** result;
	result = new int*[ax];
	for (int i = 0; i < ax; i++)
	{
		result[i] = new int[by];
	}
	for (int i = 0; i < ax; i++)
	{
		for (int j = 0; j < by; j++)
		{
			int s = 0;
			for (int l = 0; l < ay; l++)
			{
				s += a[i][l]*b[l][j];
			}
			result[i][j] = s;
		}
	}
	return result;
}

int main()
{
	int a[4] = {4,2,9,0};
	int b[4] = {3,1,-3,4};
	matriza(a,b,2,2,2);
	for(int i = 0;i < 2;i++)
	{
		for(int j = 0;j < 2;j++)
		{
			cout << c[i][j] << " " ;
		}
		cout << endl;
	}
	int g[6] = {2,1,-3,0,4,-1};
	int y[6] = {5,-1,6,-3,0,7};
	matriza(g,y,2,3,3);
	for(int i = 0;i < 3;i++)
	{
		for(int j = 0;j < 3;j++)
		{
			cout << c[i][j] << " " ;
		}
		cout << endl;
	}
}
