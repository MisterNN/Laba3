#include <iostream> 
using namespace std;

int a[100];
int b[100];
int ax , ay , by;

int** matriza(int ** a, int ** b, int ax, int ay, int by)
{
	int** result;
	result = new int*[ax];
	for (int i = 0; i < ax; i++)
	{
		result[i] = new int[by];
	}
	for (int i = 0; i < ax; i++)
	{
		for (int j = 0; j < by; j++)
		{
			int s = 0;
			for (int l = 0; l < ay; l++)
			{
				s += a[i][l]*b[l][j];
			}
			result[i][j] = s;
		}
	}
	return result;
}
void assert(int** checkout,int** matrix1,int** matrix2,int ax,int ay,int bx)
{
	int k=0;
	int** result=matriza(matrix1,matrix2,ax,ay,bx);
	for (int i=0;i < bx;i++)
	{
		for (int j=0;j< ay;j++)
		{
			if (checkout[i][j]==result[i][j])
				{
		k++;// k++ если значения совпадают,после идет проверка,если К равно количеству значений = значит все они верны,значит мастрицы перемножены праильно.
	}	
		}
	}
	if (k==bx*ay)
	{
		cout << "OK" << endl;
	}
	else
	{
		cout << "FAIL" << endl;
	}
}
int main()
{
	int** matrix1;
	matrix1 = new int * [2];
	matrix1[0] = new int[2];
	matrix1[0][0] = 4;
	matrix1[0][1] = 2;
	matrix1[1] = new int[2];
	matrix1[1][0] = 9;
	matrix1[1][1] = 0;
	int** matrix2;
	matrix2 = new int * [2];
	matrix2[0] = new int[2];
	matrix2[0][0] = 3;
	matrix2[0][1] = 1;
	matrix2[1] = new int[2];
	matrix2[1][0] = -3;
	matrix2[1][1] = 4;
	int** checkout;
	checkout = new int * [2];
	checkout[0]= new int[2];
	checkout[0][0] = 6;
	checkout[0][1] = 12;
	checkout[1] = new int[2];
	checkout[1][0] = 27;
	checkout[1][1] = 9;
	assert(checkout,matrix1,matrix2,2,2,2);
}
